<!DOCTYPE html>
<%=@controller.renderPartial("//layouts/_docHead")%>
<!-- BEGIN BODY -->
<body class="fixed-top">
	<%=@controller.renderPartial("//layouts/_headerNavbar")%>
	<!-- BEGIN CONTAINER -->
	<div class="page-container row-fluid">
		<%=@controller.renderPartial("//layouts/_pageSidebar")%>
		<%=@controller.renderPartial("//layouts/_pageContent", {content=@content})%>
	</div>
	<!-- END CONTAINER -->
	<%=@controller.renderPartial("//layouts/_footer")%>
	<%=@controller.renderPartial("//layouts/_docFooter")%>
</body>
<!-- END BODY -->
</html>
