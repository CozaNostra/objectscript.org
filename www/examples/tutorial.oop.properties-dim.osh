<pre style="font-size:18px"><%

    Matrix = {
        __construct = function(){
            this.data = []
        },
        __getdim = function(x, y){
			return this.data[y*2 + x]
        },
		__setdim = function(value, x, y){
			this.data[y*2 + x] = value
		},
    }

    var m = Matrix()
	m[1, 0] = "1_0"
	m[0, 1] = "0_1"
	print m
	print "m[1, 0] => "..m[1, 0]
	print "m[0, 1] => "..m[0, 1]
	print "m[2, 1] => "..m[2, 1]
    
%></pre>